  },
  "paths": {
    "/api/signup": {
      "post": {
        "tags": [
          "AiDrivenMedicalPlatformAPIs"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "oneOf": [
                  {
                    "$ref": "#/components/schemas/PatientRegistrationModel"
                  },
                  {
                    "$ref": "#/components/schemas/DoctorRegistrationModel"
                  },
                  {
                    "$ref": "#/components/schemas/AdminRegistrationModel"
                  }
                ]
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
